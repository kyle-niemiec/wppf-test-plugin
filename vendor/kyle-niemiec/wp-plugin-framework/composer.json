{
	"authors": [{
		"name": "Kyle Niemiec",
		"email": "kyle@codeflower.io"
	}],
	"autoload": {
		"psr-4": {
			"WPPF\\": ""
		}
	},
	"bin": ["bin/wppf"],
	"description": "My framework for facilitating WordPress plugin development.",
	"license": "GPL-3.0",
	"name": "kyle-niemiec/wp-plugin-framework",
	"require-dev": {
		"symfony/console": "^6.4",
		"code-lts/doctum": "^5.5"
	},
	"scripts": {
		"cache:clear": "rm -rf ./.doctum-cache ; mkdir ./.doctum-cache",
		"docs:clean": "rm -rf ./docs ; mkdir -p ./docs/api",
		"docs:generate": "composer docs:clean ; ./vendor/bin/doctum.php update ./doctum.php",
		"framework:version:upgrade": "./bin/wppf framework:version:upgrade"
	},
	"type": "library",
	"version": "1.2.0"
	
}
